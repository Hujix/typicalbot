{
    "version": "3.0.0-beta",
    "private": true,
    "scripts": {
        "cluster": "tsc && cd packages/typicalbot && npm run build:instance",
        "setup": "npm i && cd packages/typicalbot-cluster && npm i && cd ../typicalbot && npm i && npm run build:database",
        "start": "tsc && node production/index.js",
        "build": "tsc --watch",
        "build:database": "tsc && node production/scripts/setup.js",
        "build:docs": "esdoc",
        "build:instance": "tsc && node production/scripts/pm2.js",
        "lint": "tsc --noEmit && eslint '*/**/*.ts'",
        "lint:fix": "tsc --noEmit && eslint '*/**/*.ts' --fix"
    },
    "dependencies": {
        "canvas": "^2.6.0",
        "canvas-constructor": "^3.1.0",
        "discord.js": "github:discordjs/discord.js#master",
        "fs": "0.0.1-security",
        "i18next": "^19.0.0",
        "i18next-node-fs-backend": "^2.1.3",
        "klaw": "^3.0.0",
        "mathjs": "^6.2.1",
        "moment": "^2.22.2",
        "node-fetch": "^2.6.0",
        "node-opus": "^0.3.3",
        "pm2": "^4.1.2",
        "rethinkdb-ts": "^2.4.0-rc.16",
        "rethinkdbdash": "^2.3.31",
        "simple-youtube-api": "github:hypercoder2975/simple-youtube-api",
        "simple-youtube-stream": "github:hypercoder2975/simple-youtube-stream",
        "typescript": "^3.6.3",
        "veza": "^1.1.0",
        "vm2": "^3.8.3",
        "ytdl-core": "github:fent/node-ytdl-core"
    },
    "optionalDependencies": {
        "bignum": "^0.13.0",
        "bufferutil": "^4.0.1",
        "erlpack": "github:discordapp/erlpack",
        "utf-8-validate": "^5.0.2",
        "zlib-sync": "^0.1.5"
    },
    "devDependencies": {
        "@types/i18next-node-fs-backend": "^2.1.0",
        "@types/klaw": "^3.0.0",
        "@types/mathjs": "^6.0.2",
        "@types/node": "^12.11.1",
        "@types/node-fetch": "^2.5.1",
        "@types/ws": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^2.3.0",
        "@typescript-eslint/parser": "^2.3.0",
        "esdoc": "^1.1.0",
        "esdoc-ecmascript-proposal-plugin": "^1.0.0",
        "esdoc-standard-plugin": "^1.0.0",
        "eslint": "^6.1.0",
        "eslint-config-typicalbot": "github:typicalbot/eslint-config-typicalbot",
        "eslint-plugin-import": "^2.18.2"
    },
    "eslintConfig": {
        "extends": "eslint-config-typicalbot"
    }
}
